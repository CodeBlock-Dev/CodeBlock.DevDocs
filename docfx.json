{
  "build": {
    "content": [
      {
        "files": [
          "**/*.{md,yml}"
        ],
        "exclude": [
          "_site/**"
        ]
      }
    ],
    "resource": [
      {
        "files": [
          "images/**"
        ]
      }
    ],
    "output": "_site",
    "template": [
      "default",
      "modern",
      "template"
    ],
    "postProcessors": [
      "ExtractSearchIndex"
    ],
    "globalMetadata": {
      "_appFooter": "<span>This is a test footer</span>",
      "_appName": "CodeBlock.Dev",
      "_appTitle": "CodeBlock.Dev",
      "_enableSearch": true,
      "pdf": false,
      "_appLogoPath": "images/logo.png",
      "_appFaviconPath": "images/favicon.ico"
    }
  }
}